{
  "name": "atlantis-ui",
  "description": "framework for developing responsive, mobile first projects on the web.",
  "version": "0.0.1",
  "keywords": [
    "css",
    "less",
    "mobile-first",
    "responsive",
    "front-end",
    "framework",
    "web"
  ],
  "homepage": "https://github.com/Atlantis-Software/atlantis-ui",
  "author": "Atlantis-software",
  "scripts": {
    "build": "webpack --progress",
    "prepublish": "npm install && npm run build"
  },
  "style": "dist/css/atlantis-ui.css",
  "less": "less/atlantis-ui.less",
  "main": "./dist/js/altantis-ui.js",
  "alias": "./dist/js/ngxAtlantis-ui.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Atlantis-Software/atlantis-ui.git"
  },
  "bugs": {
    "url": "https://github.com/Atlantis-Software/atlantis-ui/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "webpack": "~2.3.2",
    "babel-core": "~6.7.0",
    "babel-loader":"~6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "css-loader":"^0.26.1",
    "style-loader":"~0.13.1",
    "less": "~2.7.1",
    "less-loader":"~2.2.3",
    "file-loader": "~0.9.0",
    "url-loader":"~0.5.8",
    "html-loader":"~0.4.5",
    "extract-text-webpack-plugin": "2.1.0",
    "@angular/core": "~2.2.0",
    "@angular/common": "~2.2.0",
    "@angular/forms": "~2.2.0",
    "rxjs": "5.0.0-rc.2"
  },
  "engines": {
    "node": ">=0.10.1"
  },
  "files": [
    "grunt",
    "js/*.js",
    "less/**/*.less",
    "LICENSE",
    "ngx/*.js"
  ],
  "jspm": {
    "main": "js/atlantis-ui",
    "shim": {
      "js/atlantis-ui": {
        "deps": "jquery",
        "exports": "$"
      }
    },
    "files": [
      "css",
      "js"
    ]
  }
}
